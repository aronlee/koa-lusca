{
  "name": "koa-lusca",
  "version": "1.1.2",
  "description": "Application security for express and koa.",
  "main": "index",
  "scripts": {
    "test": "npm run test-express && npm run test-koa",
    "test-express": "mocha --harmony -t 5000 test/*.js",
    "test-koa": "APP_MODE=koa mocha --harmony -t 5000 test/*.js",
    "test-cov": "APP_MODE=koa node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -t 5000 test/*.js",
    "test-travis": "APP_MODE=koa node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- -t 5000 test/*.js",
    "jshint": "jshint lib/ test/ index.js",
    "autod": "autod -w --prefix '~' && npm run cnpm",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org",
    "contributors": "contributors -f plain -o AUTHORS"
  },
  "homepage": "https://github.com/koajs/koa-lusca",
  "repository": {
    "type": "git",
    "url": "https://github.com/koajs/koa-lusca.git"
  },
  "author": {
    "name": "Jeff Harrell",
    "email": "jeharrell@paypal.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "engines": {
    "node": ">=0.11.x"
  },
  "engineStrict": true,
  "dependencies": {
    "koa-compose": "~2.3.0"
  },
  "devDependencies": {
    "autod": "*",
    "contributors": "*",
    "express": "~4.9.0",
    "istanbul-harmony": "*",
    "jshint": "*",
    "koa": "~0.11.0",
    "koa-middlewares": "~1.2.0",
    "mocha": "*",
    "pedding": "~1.0.0",
    "supertest": "~0.13.0",
    "body-parser": "^1.6.3",
    "cookie-parser": "^1.3.2",
    "cookie-session": "^1.0.2",
    "data-driven": "^1.0.0",
    "errorhandler": "^1.1.1",
    "express-session": "^1.7.5"
  }
}
